{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "cooee-admin": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/cooee-admin",
            "sourceRoot": "projects/cooee-admin/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/cooee-admin",
                        "index": "projects/cooee-admin/src/index.html",
                        "main": "projects/cooee-admin/src/main.ts",
                        "polyfills": "projects/cooee-admin/src/polyfills.ts",
                        "tsConfig": "projects/cooee-admin/tsconfig.app.json",
                        "aot": true,
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "projects/cooee-core/src"
                            ]
                        },
                        "assets": [
                            "projects/cooee-admin/src/favicon.ico",
                            "projects/cooee-admin/src/assets"
                        ],
                        "styles": [
                            "projects/cooee-admin/src/styles.scss",
                            "node_modules/ziggeo-client-sdk/build/ziggeo.css"
                        ],
                        "scripts": [
                            "node_modules/ziggeo-client-sdk/build/ziggeo.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/cooee-admin/src/environments/environment.ts",
                                    "with": "projects/cooee-admin/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "50kb",
                                    "maximumError": "100kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "cooee-admin:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "cooee-admin:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "cooee-admin:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/cooee-admin/src/test.ts",
                        "polyfills": "projects/cooee-admin/src/polyfills.ts",
                        "tsConfig": "projects/cooee-admin/tsconfig.spec.json",
                        "karmaConfig": "projects/cooee-admin/karma.conf.js",
                        "assets": [
                            "projects/cooee-admin/src/favicon.ico",
                            "projects/cooee-admin/src/assets"
                        ],
                        "styles": [
                            "projects/cooee-admin/src/styles.scss"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/cooee-admin/tsconfig.app.json",
                            "projects/cooee-admin/tsconfig.spec.json",
                            "projects/cooee-admin/e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "projects/cooee-admin/e2e/protractor.conf.js",
                        "devServerTarget": "cooee-admin:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "cooee-admin:serve:production"
                        }
                    }
                }
            }
        },
        "cooee-client": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/cooee-client",
            "sourceRoot": "projects/cooee-client/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "projects/cooee-core/src"
                            ]
                        },
                        "outputPath": "dist/cooee-client",
                        "index": "projects/cooee-client/src/index.html",
                        "main": "projects/cooee-client/src/main.ts",
                        "polyfills": "projects/cooee-client/src/polyfills.ts",
                        "tsConfig": "projects/cooee-client/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "projects/cooee-client/src/favicon.ico",
                            "projects/cooee-client/src/assets"
                        ],
                        "styles": [
                            "projects/cooee-client/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/cooee-client/src/environments/environment.ts",
                                    "with": "projects/cooee-client/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "cooee-client:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "cooee-client:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "cooee-client:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/cooee-client/src/test.ts",
                        "polyfills": "projects/cooee-client/src/polyfills.ts",
                        "tsConfig": "projects/cooee-client/tsconfig.spec.json",
                        "karmaConfig": "projects/cooee-client/karma.conf.js",
                        "assets": [
                            "projects/cooee-client/src/favicon.ico",
                            "projects/cooee-client/src/assets"
                        ],
                        "styles": [
                            "projects/cooee-client/src/styles.scss"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/cooee-client/tsconfig.app.json",
                            "projects/cooee-client/tsconfig.spec.json",
                            "projects/cooee-client/e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "projects/cooee-client/e2e/protractor.conf.js",
                        "devServerTarget": "cooee-client:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "cooee-client:serve:production"
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "cooee-admin"
}
